<?xml version="1.0"?>
<csp_whitelist xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
               xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Csp/etc/csp_whitelist.xsd">
    <policies>
        <policy id="script-src">
            <values>
                <value id="unpkg" type="host">https://unpkg.com</value>
                <value id="polyfill" type="host">https://polyfill.io</value>
                <value id="googleapis" type="host">https://maps.googleapis.com</value>
            </values>
        </policy>
        <policy id="connect-src">
            <values>
                <value id="googleapisConnect" type="host">https://maps.googleapis.com</value>
            </values>
        </policy>
        <policy id="style-src">
            <values>
                <value id="fontsGoogleapis" type="host">https://fonts.googleapis.com/</value>
            </values>
        </policy>
        <policy id="img-src">
            <values>
                <value id="mapsGstatic" type="host">https://maps.gstatic.com</value>
                <value id="imgGoogleapis" type="host">https://maps.googleapis.com</value>
                <value id="imgGoogleapis" type="host">https://maps.googleapis.com</value>
            </values>
        </policy>
        <policy id="font-src">
            <values>
                <value id="fontsGstatic" type="host">https://fonts.gstatic.com</value>
            </values>
        </policy>
    </policies>
</csp_whitelist>
